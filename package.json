{
    "name": "orochi-ui",
    "version": "0.0.1",
    "description": "Orochi UI",
    "author": "https://themeforest.net/user/srcn",
    "license": "https://themeforest.net/licenses/standard",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "NODE_OPTIONS=--max-old-space-size=5120 ng serve -c en --configuration=hmr",
        "start-te": "NODE_OPTIONS=--max-old-space-size=5120 ng serve -c te",
        "start-ja": "NODE_OPTIONS=--max-old-space-size=5120 ng serve -c ja",
        "build": "NODE_OPTIONS=--max-old-space-size=5120 ng build --configuration production",
        "build-dashboard": "./build-prefix-structure.sh dashboard",
        "build-prefix": "./build-prefix-structure.sh",
        "watch": "NODE_OPTIONS=--max-old-space-size=5120 ng build --watch --configuration development",
        "translations": "node --max-old-space-size=5120 ./node_modules/@angular/cli/bin/ng extract-i18n --output-path src/locale --format=xlf2 --out-file messages.en.xlf2",
        "xlf-translate": "node translate/translate2.js --i18nPath=src/locale"
    },
    "dependencies": {
        "@angular/animations": "^19.0.0",
        "@angular/cdk": "^19.0.0",
        "@angular/common": "^19.0.0",
